{
  "name": "englishforkids",
  "version": "1.0.0",
  "description": "RSSchool personal task",
  "private": true,
  "scripts": {
    "lint": "eslint src/scripts/**/*.js && stylelint src/styles/**/*.scss --syntax scss",
    "deploy": "npm run lint && npm run build && netlify deploy --prod",
    "start": "webpack-dev-server --open",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tensegrity666/english-for-kids.git"
  },
  "keywords": [
    "rsschool",
    "js",
    "javascript"
  ],
  "author": "Roman Yakovlev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tensegrity666/english-for-kids/issues"
  },
  "homepage": "https://github.com/tensegrity666/english-for-kids#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.0",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
